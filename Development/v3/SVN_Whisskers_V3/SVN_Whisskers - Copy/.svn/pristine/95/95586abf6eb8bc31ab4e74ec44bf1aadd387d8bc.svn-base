<script type="text/javascript">


$(function(){
  //allow the validation framework to re-prase the DOM
  jQuery.validator.unobtrusive.parse();

  //or to give the parser some context, supply it with a selector
  //jQuery validator will parse all child elements (deep) starting
  //from the selector element supplied
  jQuery.validator.unobtrusive.parse("#formCampaign");

  $("#btnCreateCamp").click(function () {

      if (!$("formCampaign").valid()) {
          return false;
      }
  });


  jQuery.validator.unobtrusive.parse();
  jQuery.validator.unobtrusive.parse("#formAdgroups");
  $("#btnCreateAdgroups").click(function () {
      if (!$("formAdgroups").valid()) {
          return false;
      }
  });

  jQuery.validator.unobtrusive.parse();
  jQuery.validator.unobtrusive.parse("#formAds");
  $("#btnCreateAds").click(function () {
      if (!$("formAds").valid()) {
          return false;
      }
  });

});

function textCounter(field, field2, maxlimit) {
    var countfield = document.getElementById(field2);
    if (field.value.length > maxlimit) {

        //alert(maxlimit - field.value.length);
        countfield.value = maxlimit - field.value.length;
        if (countfield.value <= -1)
        {
            $(field2).css("color","red");
        }
        field.value = field.value.substring(0, maxlimit);
        //$("msgvalid14").html("You have reach the limit.")
        countfield.value = countfield.value + " You have reach the limit.";
       // alert(field.value);
        //alert("------" + field.val())


       // $('#printchatbox').innerHTML = field.value;
        return false;

    } else {
        countfield.value = maxlimit - field.value.length;
        //$("msgvalid14").html('');
    }


}
