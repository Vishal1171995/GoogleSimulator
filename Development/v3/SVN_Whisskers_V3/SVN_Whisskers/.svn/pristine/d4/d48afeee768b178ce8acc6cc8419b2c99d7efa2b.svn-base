    function BindStudentCampaignTab() {
        debugger;
        var UrlLength = window.location.href.toString().split("#").length;
        if (window.location.href.toString().split("#")[1].toUpperCase() == "CAMPAIGNS" && UrlLength == 2) {
            getCampaignsOnDemand();
        }
        if (window.location.href.toString().split("#")[1].toUpperCase() == "ADGROUPS" && UrlLength == 2) {
            getADGroupsOnDemand();
        }
        if (window.location.href.toString().split("#")[1].toUpperCase() == "ADS" && UrlLength == 2) {
            getCampaignAdsOnDemand();
        }
        if (window.location.href.toString().split("#")[1].toUpperCase() == "KEYWORDS" && UrlLength == 2) {
            getMappedkeyWordsOnDemand();
        }
        CheckStudentRFAStatus();
    }
    function closeCampaignPopup(data) {
        debugger;
        var JsonObj = data[0];
        $('#divMsgNotify').text(data[0]);
        fadeOut("divMsgNotify");
        if (data[0] == "0") {
            $('#hdnCampId').val('0');
            $('#SuccessHead').text('You have created Campaign successFully');
        }
        if (data[0] == "1") {
            $('#hdnCampId').val('0');
            $('#SuccessHead').text('You have updated Campaign successfully');
        }
        $("#btnCreateCamp").attr("disabled", false);
        if (data[1] == "1") {
            ClearCampaignPopup();
            BindStudentCampaignTab();
            $('#ModalSuccess').modal('show');
            $('#hdnCampId').val('0');
        }
    }
    function CloseAdgroupPopup(data) {
        debugger;
        var JsonObj = data[0];
        $('#divMsgNotify').text(data[0]);
        fadeOut("divMsgNotify");
        if (data[0] == "0") {
