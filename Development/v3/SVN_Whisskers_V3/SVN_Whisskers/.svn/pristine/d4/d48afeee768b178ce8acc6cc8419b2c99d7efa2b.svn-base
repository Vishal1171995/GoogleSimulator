<style>
    .ui-autocomplete {
        z-index: 9999999 !important;
        position: absolute;
        margin: 0px;
        padding: 0px;
        height: 200px;
        overflow: scroll;
        overflow-x: hidden;
        border: 1px solid #d5d5d5;
    }

        .ui-autocomplete li {
            list-style-type: none;
            background: rgba(255,255,255,0.9) !important;
            padding: 5px 12px;
            color: #666;
            font-size: 14px;
            cursor: pointer;
        }

    .printchatbox {
        border-width: thick 10px;
        border-style: solid;
        background-color: #fff;
        line-height: 2;
        color: #6E6A6B;
        font-size: 14pt;
        text-align: left;
        float: middle;
        border: 3px solid #969293;
        width: 40%;
        height: 2em;
    }
</style>
